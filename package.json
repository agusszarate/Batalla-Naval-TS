    "description": "Juego de Batalla Naval multijugador",
    "main": "dist/server/server.js",
    "scripts": {
        "build": "webpack --config webpack.config.js",
        "start": "node dist/server/server.js",
        "dev:build": "webpack --config webpack.config.js --watch",
        "dev:server": "nodemon --delay 1 --watch dist dist/server/server.js",
        "dev": "concurrently \"npm run dev:build\" \"npm run dev:server\"",
        "test": "echo \"Error: no test specified\" && exit 1"
    },
    "keywords": [
        "batalla-naval",
        "juego",
        "sockets",
        "typescript"
    ],

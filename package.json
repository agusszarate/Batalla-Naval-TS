    "dependencies": {
        "@emotion/react": "^11.14.0",
        "@emotion/styled": "^11.14.0",
        "@mui/icons-material": "^7.0.1",
        "@mui/material": "^7.0.1",
        "express": "^4.18.2",
        "socket.io": "^4.7.2",
        "socket.io-client": "^4.7.2"
    },
    "devDependencies": {
        "@types/express": "^4.17.21",
        "@types/node": "^20.10.0",
        "concurrently": "^8.2.2",
        "copy-webpack-plugin": "^11.0.0",
        "nodemon": "^3.0.1",
        "ts-loader": "^9.5.1",
        "typescript": "^5.3.2",
        "webpack": "^5.89.0",
